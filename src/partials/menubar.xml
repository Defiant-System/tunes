<MenuBar>
	<Menu name="Tunes">
		<Menu name="About Tunes" shell="win -a"/>
		<Menu type="divider"/>
		<Menu name="Quit Tunes" shell="win -c"/>
	</Menu>
	<Menu name="Playback">
		<Menu name="Play" click="play-play"/>
		<Menu type="divider"/>
		<Menu name="Next" click="play-next"/>
		<Menu name="Previous" click="play-previous"/>
		<Menu name="Shuffle" click="play-random" is-checked="0"/>
		<Menu name="Repeat" click="play-repeat" is-checked="0"/>
		<Menu type="divider"/>
		<Menu name="Volume Up" click="volume-up"/>
		<Menu name="Volume Down" click="volume-down"/>
	</Menu>
	<Menu name="Help">
		<Menu name="Send Tunes Feedback"/>
		<Menu type="divider"/>
		<Menu name="Tunes Help"/>
		<Menu type="divider"/>
		<Menu name="License"/>
	</Menu>
</MenuBar>