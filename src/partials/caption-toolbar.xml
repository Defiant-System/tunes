<CaptionToolBar has="display">
	<Group class="tools-left">
		<Options>
			<Tool symbol="icon-previous" click="previous"/>
			<Tool symbol="icon-next" click="next"/>
			<Tool symbol="icon-pause" click="pause"/>
			<Tool symbol="icon-play" click="play"/>
		</Options>
	</Group>
	<Group class="display">
		<Field>
			
		</Field>
		<Canvas/>
